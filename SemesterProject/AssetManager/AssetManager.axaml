<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:SemesterProject"
        Title="Asset Manager" Width="800" Height="600">
	<Grid>
		<DataGrid ItemsSource="{Binding ProductionUnits}" AutoGenerateColumns="False">
			<DataGrid.Columns>
				<DataGridTextColumn Header="Name" Binding="{Binding Name}" />
				<DataGridTextColumn Header="Max Heat (MW)" Binding="{Binding MaxHeat}" />
				<DataGridTextColumn Header="Max Electricity (MW)" Binding="{Binding MaxElectricity}" />
				<DataGridTextColumn Header="Production Costs (DKK/MWh(th))" Binding="{Binding ProductionCosts}" />
				<DataGridTextColumn Header="CO2 Emissions (kg/MWh(th))" Binding="{Binding CO2Emissions}" />
				<DataGridTextColumn Header="Fuel Type" Binding="{Binding FuelType}" />
			</DataGrid.Columns>
		</DataGrid>
	</Grid>
</Window>
